import os
os.system("cls")

print("NESTED LOOP DENGAN FOR")
for a in range(4):
    for i in range(5):
        print(a,i)

print("NESTED LOOP DENGAN WHILE")
i = 1
while i<=3:
    print(i)
    j = 5
    while j <= 8:
        print("Nilai i =",i)
        print("Nilai j =", j)
        j+=1
    i+=1
    
    
